{
    "compilerOptions": {
        "target": "es5",
        "lib": [
            "dom",
            "dom.Iterable",
            "esnext",
        ],
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "jsx": "react",
        "rootDir": "src",
        "baseUrl": "src",
        "paths": {
            "@/*": ["*"]
        },
        "allowJs": true,
        "skipDefaultLibCheck": true,
        "allowSyntheticDefaultImports": true,
        "strict": false,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "noEmit": true
    },
    "include": [
        "src"
    ]
}